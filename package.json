{
  "name": "mozaik-ext-airtable",
  "version": "0.0.1",
  "description": "Mozaik airtable widgets",
  "repository": {
    "type": "git",
    "url": "git://github.com/b5/mozaik-ext-airtable"
  },
  "license": "MIT",
  "author": {
    "name": "Brendan O'Brien",
    "url": "https://airtable.com/b5"
  },
  "homepage": "https://github.com/b5/mozaik-ext-airtable",
  "main": "./lib/components/index.js",
  "keywords": [
    "airtable",
    "mozaik",
    "widget",
    "extension",
    "dashboard"
  ],
  "engineStrict": true,
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.0.0"
  },
  "dependencies": {
    "babel-cli": "^6.18.0",
    "bluebird": "^3.4.7",
    "chalk": "1.1.3",
    "convict": "^1.5.0",
    "lodash": "^4.17.2",
    "moment": "^2.17.1",
    "moment-duration-format": "1.3.0",
    "superagent": "^3.3.1",
    "superagent-bluebird-promise": "^4.1.0"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "coveralls": "^2.11.15",
    "enzyme": "^2.7.0",
    "eslint": "^3.12.2",
    "eslint-config-react-app": "^0.5.0",
    "eslint-plugin-flowtype": "^2.29.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.8.0",
    "mockery": "^2.0.0",
    "mozaik": "^2.0.0-alpha.11",
    "nivo": "^1.0.0-alpha.8",
    "nock": "^9.0.2",
    "nyc": "^10.0.0",
    "react": "^15.4.1",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.4.1",
    "sinon": "^1.17.6"
  },
  "peerDependencies": {
    "mozaik": "^2.0.0-alpha.11",
    "nivo": "^1.0.0-alpha.8",
    "react": "^15.4.1"
  },
  "scripts": {
    "eslint": "eslint ./src/** ./test/**",
    "test": "ava --verbose",
    "test-cover": "nyc ava",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "cover-report": "nyc report --reporter=lcov && open coverage/lcov-report/index.html",
    "build": "rm -rf lib/* && babel src --no-comments --out-dir lib",
    "watch": "babel src --watch --out-dir lib"
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "tap": false,
    "failFast": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "nyc": {}
}
